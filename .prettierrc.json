{
    "singleQuote": false,
    "printWidth": 200,
    "proseWrap": "preserve",
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "embeddedLanguageFormatting": "auto",
    "arrowParens": "avoid",
    "styleLintIntegration": true,
    "plugins": ["prettier-plugin-tailwindcss"]
}
